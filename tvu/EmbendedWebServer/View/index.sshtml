    <table id="location" border='1'>
        <tr>
            <th>Title</th>
            <th>Total Down</th>
			<th>Price</th>
        </tr>
@Each.Products
        <tr>
            <th>@Current.Id</th>
            <th>@Current.Name</th>
			<th>@Current.Price</th>
			<td><a href="/delete/@Current.Id">Delete With JavaScript</a></td>
			<td><a href="/delete/@Current.Id">Delete Without JavaScript</a></td>
        </tr>
@EndEach
    </table>

	@Model.Deleted<br>
	<br>
	<br>
	@Model[.Products.ID]

	<br>
	<br><br>
@foreach (var product in Model.Products)
  {
    <tr>
      <td>@product.Id</td>
      <td>@product.Name</td>
      <td>@product.Price.ToString("c")</td>
      <td><a href="/delete/@product.Id">Delete With JavaScript</a></td>
      <td><a href="/delete/@product.Id">Delete Without JavaScript</a></td>
    </tr>  
  }

